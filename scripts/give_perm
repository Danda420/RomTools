#!/bin/bash
cd ..
DIRTOOLS=$PWD
cd scripts
DIRROM=$DIRTOOLS/ROM

sudo chown -R $USER:$USER $DIRROM/system >> log.txt
sudo chown -R $USER:$USER $DIRROM/vendor >> log.txt
sudo chown -R $USER:$USER $DIRROM/odm >> log.txt
sudo chown -R $USER:$USER $DIRROM/system_ext >> log.txt
sudo chown -R $USER:$USER $DIRROM/product >> log.txt
